border-right:1px solid #999999;
border-top:1px solid #999999;
font-weight:normal;
padding:0 1em;
text-transform:uppercase;
}
.listing .top {
border-left:1px solid #FFFFFF;
border-right:1px solid #FFFFFF !important;
border-top:1px solid #FFFFFF !important;
padding:0 0 1em;
