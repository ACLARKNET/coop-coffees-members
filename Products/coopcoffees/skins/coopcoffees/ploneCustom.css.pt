}
.stx table p {
margin:0;
padding:0;
}
.stx table {
border:1px solid #999999 !important;
}
.stx table td {
border-bottom:1px solid #999999;
}
.portlet {
border:medium none;
font-family:Arial,Verdana,Lucida,Helvetica,Arial,sans-serif;
margin-bottom:0 !important;
padding:0 !important;
}
#region-content .portlet {
font-size:94%;
}
.portlet img {
