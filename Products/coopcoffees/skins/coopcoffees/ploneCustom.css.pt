border:1px solid #999999;
float:left;
margin:1em;
padding:1em 0;
text-align:center;
width:110px;
}
.portrait {
background-color:#FFFFFF;
border:1px solid #999999;
font-size:85%;
margin:0.5em;
padding:1em 0 0.5em;
text-align:center;
width:100px;
}
.portraitPhoto {
